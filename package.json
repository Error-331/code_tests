{
    "name": "CodeTest",
    "version": "0.0.1",
    "description": "code testing platform...",
    "main": "index.js",
    "scripts": {
        "run-scripts": "node ./utils/compile_scripts.js --type=vanilla && node ./dist/bundle.js",
        "run-scripts-browser": "node ./utils/compile_all_browser_js.js && node ./src/browser_js/server.js",
        "run-scripts-nodejs": "node ./src/node_js/index.js",
        "run-scripts-ts": "node ./utils/compile_scripts.js --type=type_script && node ./dist/typescript_bundle.js",
        "run-scripts-tools": "node ./utils/compile_scripts.js --type=tools && node ./dist/tools_bundle.js",
        "run-scripts-playground": "node ./utils/compile_scripts.js --type=playground && node ./dist/playground_bundle.js",
        "run-scripts-vendor-library-tests": "node ./utils/compile_scripts.js --type=vendor_library_tests && node ./dist/vendor_library_tests_bundle.js",
        "run-scripts-databases": "node ./src/databases/index.js --os=linux",
        "compile-browser-scripts": "node ./utils/compile_all_browser_js.js",
        "watch-compile-clojure-scripts": "java -cp cljs.jar:src/clojure_script clojure.main ./build.clj"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "7.4.3",
        "@babel/core": "7.4.3",
        "@babel/preset-env": "7.4.3",
        "@babel/plugin-proposal-decorators": "7.4.4",
        "@babel/plugin-proposal-class-properties": "7.4.4",
        "core-js": "3.1.2",
        "babel-loader": "8.0.6",
        "elm": "^0.18.0",
        "json-loader": "^0.5.7",
        "jsonfile": "^4.0.0",
        "minimist": "^1.2.0",
        "rimraf": "^2.6.2",
        "ts-loader": "^2.3.7",
        "tslib": "^1.9.0",
        "typescript": "^2.6.2",
        "typings": "^2.1.1",
        "webpack": "^3.10.0",
        "worker-loader": "^0.7.1"
    },
    "dependencies": {
        "fluture": "^5.0.0",
        "lodash": "4.17.15",
        "moment": "^2.20.1",
        "moment-timezone": "^0.5.14",
        "mongodb": "^2.2.34",
        "mongoose": "^4.13.10",
        "sqlite": "3.0.3",
        "mori": "^0.3.2",
        "nedb": "^1.8.0",
        "ramda": "^0.26.0",
        "crocks": "0.11.1",
        "folktale": "^2.1.0",
        "redis": "^2.8.0",
        "sanctuary": "^0.13.2",
        "rxjs": "6.5.2",
        "semver": "6.3.0",
        "chalk": "2.4.2",
        "gl-matrix": "^2.6.1"
    }
}
